<!DOCTYPE html>
<html>
<head>
  <title>Annotation</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, msg| %>
    <div id="flash_<%= name %>" class="flash_messages">
      <%= msg %>
      <%= link_to "hide message", nil, class: "hide" %>
    </div>
  <% end %>
  </div>
  <div id="container">
    <%= yield %>
  </div>
</body>
</html>
